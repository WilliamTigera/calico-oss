{
  "job_id": "$CLUSTER_NAME.outbound_service_bytes_anomaly",
  "job_type": "anomaly_detector",
  "job_version": "6.4.3",
  "groups": [
    "dos"
  ],
  "description": "Outbound service bytes anomaly",
  "analysis_config": {
    "bucket_span": "10m",
    "detectors": [
      {
        "detector_description": "Output bytes spike for a pod within a replica set",
        "function": "high_sum",
        "field_name": "bytes_out",
        "over_field_name": "source_name",
        "partition_field_name": "namespace_source_name_aggr",
        "detector_index": 0
      }
    ],
    "influencers": [
      "source_name",
      "source_name_aggr",
      "source_namespace"
    ]
  },
  "analysis_limits": {
    "model_memory_limit": "1024mb",
    "categorization_examples_limit": 4
  },
  "data_description": {
    "time_field": "start_time",
    "time_format": "epoch_ms"
  },
  "model_snapshot_retention_days": 1,
  "custom_settings": {
    "created_by": "population-wizard"
  },
  "results_index_name": "service_bytes_anomaly.$CLUSTER_NAME"
}
