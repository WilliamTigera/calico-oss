{
  "job_id": "$CLUSTER_NAME.inbound_connection_spike",
  "job_type": "$CLUSTER_NAME.anomaly_detector",
  "job_version": "6.4.3",
  "groups": [
    "dos"
  ],
  "description": "Inbound connection spike",
  "analysis_config": {
    "bucket_span": "10m",
    "detectors": [
      {
        "detector_description": "Inbound connection spike for a pod within a replica set",
        "function": "high_sum",
        "field_name": "num_flows_started",
        "over_field_name": "dest_name",
        "partition_field_name": "namespace_dest_name_aggr",
        "detector_index": 0
      },
      {
        "detector_description": "Inbound connection spike over a whole replica set",
        "function": "high_non_null_sum",
        "field_name": "num_flows_started",
        "partition_field_name": "namespace_dest_name_aggr",
        "detector_index": 1
      }
    ],
    "influencers": [
      "dest_name",
      "dest_name_aggr",
      "dest_namespace"
    ]
  },
  "analysis_limits": {
    "model_memory_limit": "1024mb",
    "categorization_examples_limit": 4
  },
  "data_description": {
    "time_field": "start_time",
    "time_format": "epoch_ms"
  },
  "model_snapshot_retention_days": 1,
  "custom_settings": {
    "created_by": "population-wizard"
  },
  "results_index_name": "inbound_connection_spike.$CLUSTER_NAME"
}
